void setup()
{
pinMode(10,OUTPUT);
pinMode(11,OUTPUT);

digitalWrite(10,HIGH);          //FORWARD
digitalWrite(11,LOW);
delay(2000);

digitalWrite(10,LOW);          //BACKWARD
digitalWrite(11,HIGH);
delay(2000);


digitalWrite(10,HIGH);      //STOP
digitalWrite(11,HIGH);
delay(5000);
}

void loop()
{

}
